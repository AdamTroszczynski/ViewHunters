generator client {
  provider = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Places {
  id Int @id @default(autoincrement())
  name String
  geoWidth Int
  geoHeight Int
  photo String
  category String
  description String
  code String
}
