  "scripts": {
    "test": "dotenv -e .env.test.local -- jest",
    "test:cov": "dotenv -e .env.test.local -- jest --coverage",
    "start": "npm-run-all --parallel dev:server dev:build",
    "build:prod": "webpack build --progress --color",
    "dev:build": "webpack --watch",
    "dev:server": "webpack && nodemon \"./dist/server.js\" --watch \"./dist\"",
    "format": "prettier --write \"src/**/*.+(js|ts)\"",
    "prepare": "cd ../ && husky ./server/.husky",
    "lint-staged": "lint-staged",
    "prisma:push": "prisma db push",
    "prisma:generate": "prisma generate"
  },
  "lint-staged": {
    "*.{js, ts}": [
      "eslint --quiet --fix",
      "prettier --write --ignore-unknown"
    ]
  },
